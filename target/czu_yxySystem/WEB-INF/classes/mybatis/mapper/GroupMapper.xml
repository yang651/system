<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.czu.yxySystem.mapper.GroupMapper">

    <select id="getList" resultType="com.czu.yxySystem.entity.DO.GroupDO">
        select * from tb_group
        <where>
            <if test="jueSeName != '' and jueSeName != null">
                groupName like '%' + #{jueSeName} +'%'
            </if>
        </where>
    </select>
</mapper>